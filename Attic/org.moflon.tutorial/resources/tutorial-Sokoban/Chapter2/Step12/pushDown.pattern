pattern pushDown {
	
	oldFloor : Floor { 
		-bottom-> floor
	}
	
	@floor : Floor { 
		-bottom-> neighbor
	}
	
	@this : Figure { 
		-- -floor-> oldFloor
		++ -floor-> floor
	}
	
	server : Server { 
		-- -floor-> floor
		++ -floor-> neighbor
	}
	
	neighbor : Floor
	
	! noWall : Wall { 
		 -floor-> neighbor
	}

}