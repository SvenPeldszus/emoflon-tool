group mconf;


constraint(opposites, foreign) ::= <<
<opposites><if (foreign)>
<\n><foreign><endif> >>

opposites(opposites) ::= <<
opposites {
	<opposites; separator="\n">
}
>>

oppositeRelationship(src, srcType, trg, trgType) ::= <<
	<src> : <srcType> \<-\> <trg> : <trgType>
>>

foreign(export, uri, prefix) ::= <<
foreign {	
	export := <export>
	uri := '<uri>' 
	prefix := '<prefix>'	
}
>>

imports(imports) ::= "<imports>"

importValue(importValue) ::= "import <importValue><\n>"