pattern pushDown {
	
	oldFloor : Floor { 
		-bottom-> floor
	}
	
	@floor : Floor { 
		-bottom-> neighbour
	}
	
	@this : Figure { 
		-- -floor-> oldFloor
		++ -floor-> floor
	}
	
	server : Server { 
		-- -floor-> floor
		++ -floor-> neighbour
	}
	
	neighbour : Floor
	
	! noWall : Wall { 
		 -floor-> neighbour
	}

}