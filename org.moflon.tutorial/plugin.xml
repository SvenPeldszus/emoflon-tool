<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.cheatsheets.cheatSheetContent">
      <category
            id="org.moflon.tutorial.category"
            name="eMoflon Tutorial">
      </category>
      <cheatsheet
            category="org.moflon.tutorial.category"
            composite="true"
            contentFile="compositecheatsheet.xml"
            id="org.moflon.tutorial.cheatsheet_composite"
            name="eMoflon Tutorial">
      </cheatsheet>
   </extension>
   <extension
         point="org.eclipse.ui.intro.configExtension">
      <configExtension
            configId="org.eclipse.ui.intro.universalConfig"
            content="intro/sample.xml">
      </configExtension>
   </extension>
   <extension
         point="org.eclipse.ui.cheatsheets.cheatSheetItemExtension">
      <itemExtension
            class="org.moflon.tutorial.validation.ItemExtension"
            itemAttribute="validation">
      </itemExtension>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.moflon.examples.category"
            name="eMoflon Handbook Examples"
            parentCategory="org.eclipse.ui.Examples">
      </category>
      <wizard
            canFinishEarly="true"
            category="org.eclipse.ui.Examples/org.moflon.examples.category"
            class="org.moflon.handbook.examples.WizardPart2GUI"
            finalPerspective="org.moflon.ide.ui.perspective"
            hasPages="false"
            icon="resources/icons/moflonSmall.png"
            id="org.moflon.handbook.example.newwizard.part1"
            name="Part II GUI Download"
            project="true">
      </wizard>
      <wizard
            canFinishEarly="true"
            category="org.eclipse.ui.Examples/org.moflon.examples.category"
            class="org.moflon.handbook.examples.WizardPart3Textual"
