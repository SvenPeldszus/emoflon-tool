group Sch;

schema(source, target, classes) ::= <<
source /<source>
target /<target>

<classes>
>>

classes(classes) ::= <<
<classes; separator="\n">
>>

class(name, references) ::= <<
class <name> {
	<references>
}
>>

references(source, target) ::= <<
source -> <source>
target -> <target>
>>

value(value) ::= <<
<value>
>>